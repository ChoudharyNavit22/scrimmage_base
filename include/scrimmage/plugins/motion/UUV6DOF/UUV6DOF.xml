       Model for the REMUS Autonomous Underwater Vehicle" by Timothy Prestero
  -->

  <surface_height>0</surface_height>

  <!-- kg * m^2 -->
  <inertia_matrix>
    [ 0.177 0.000 0.000 ]
    [ 0.000 3.450 0.000 ]
    [ 0.000 0.000 3.450 ]
