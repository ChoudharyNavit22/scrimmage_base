<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="http://gtri.gatech.edu"?>
<params>
  <library>GroundCollision_plugin</library>  
  
  <!-- if defined, ground_collision_altitude has priority -->
  <!--<ground_collision_altitude>300</ground_collision_altitude>-->
  <ground_collision_z>0</ground_collision_z> <!-- uses altitude origin -->

  <!-- Should collision detection occur before instantiating entity? -->
  <enable_startup_collisions>true</enable_startup_collisions>

  <!-- Should the entity be removed on a collision or should a ground force be
       applied to the motion model. Note: the motion model must make use of
       external forces when remove_on_collision is set to false. Otherwise,
       nothing will happen. -->
  <remove_on_collision>true</remove_on_collision>  
  <team>all</team>
  
</params>
